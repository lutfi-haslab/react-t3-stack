{
  "name": "react-t3-stack",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "React Tanstack, Telefunc, and Tailwind Stack",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev": "npm run server:dev",
