{
  "name": "react-t3-stack",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "React Tanstack, Telefunc, and Tailwind Stack",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev": "npm run server:dev",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build && vite build --ssr",
    "server:dev": "nodemon ./server.ts",
    "server:prod": "cross-env NODE_ENV=production tsx ./server.ts",
    "test": "tsx ./test/index.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.0-alpha.8",
    "@tanstack/react-router": "^1.19.8",
    "express": "^4.18.3",
    "nodemon": "^3.1.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.0.0-alpha.8",
    "telefunc": "^0.1.71"
  },
  "devDependencies": {
    "@tanstack/router-vite-plugin": "^1.19.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.26",
    "@types/pg": "^8.11.2",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  }
}
